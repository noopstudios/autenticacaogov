{
    "name": "noopstudios/autenticacaogov",
    "description": "Package used to allow laravel website to communicate with AutenticacaoGov with OAUTH2.0 protocolo (implicit flow)",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "laravel/socialite": "^5.1.0"
    },
    "autoload": {
        "psr-4": {
            "Noop\\AutenticacaoGov\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noop\\AutenticacaoGov\\AutenticacaoGovServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noop",
            "email": "dev@noop.pt"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
